@page
@model EDTProjectM1.DeleteUEs

@{
    ViewData["Title"] = "Delete";
}

<h1>Supprimer</h1>

<h3>Êtes-vous sûr de vouloir supprimer cette UE ?</h3>
<div>
    <h4>UE</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.UE.Intitule)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UE.Intitule)
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="UE.Numero" />
        <input type="submit" value="Supprimer" class="btn btn-danger" /> |
        <a asp-page="./Index">Retourner à la liste des UEs</a>
    </form>
</div>
